{
  "name": "xebra.js",
  "version": "1.0.0",
  "description": "Xebra connects Max to the browser, and the browser to Max.",
  "author": {
    "name": "Cycling '74",
    "url": "http://cycling74.com"
  },
  "contributors": [
    "Florian Demmer <florian@cycling74.com>",
    "Sam Tarakajian <sam@cycling74.com>",
    "Jeremy Bernstein <jeremy@cycling74.com>",
    "Ben Bracken <ben@cycling74.com>",
    "Cassie Tarakajian <cassie@cycling74.com>"
  ],
  "main": "lib/index.js",
  "homepage": "http://cycling74.com",
  "readme": "README.md",
  "bugs": "https://github.com/Cycling74/xebra.js/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cycling74/xebra.js.git"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "node ./.bin/build.js",
    "predocs": "rimraf ./docs",
    "docs": "sh ./.bin/docs",
    "prelib": "rimraf ./lib",
    "lib": "babel ./src -d ./lib",
    "lint": "eslint src -c ./.eslintrc.json",
    "test": "npm run lint",
    "preversion": "npm run test",
    "version": "npm run lib && npm run build",
    "publish:patch": "npm version patch --no-git-tag-version && echo 'add actual publish here'",
    "publish:minor": "npm version minor --no-git-tag-version && echo 'add actual publish here'",
    "publish:major": "npm version major --no-git-tag-version && echo 'add actual publish here'",
    "postpublish": "sh ./.bin/docs && node ./.bin/publish_docs.js"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "lodash.pick": "^4.4.0",
    "xebra-communicator": "git+ssh://github.com/Cycling74/xebra-communicator#master"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.12.1",
    "gh-pages": "^0.12.0",
    "jaguarjs-jsdoc": "git://github.com/fde31/jaguarjs-jsdoc#xebra_docs",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "rimraf": "2.5.4",
    "uglify-js": "^2.7.3"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
